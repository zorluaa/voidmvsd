local fovEnabled = false
local fovValue = 70

local function ChangeFOV()
	while fovEnabled do
		game.Workspace.CurrentCamera.FieldOfView = fovValue
		wait(0.1)
	end
end

Tab3:CreateToggle({
	Name = "Enable/Disable FOV",
	CurrentValue = false,
	Flag = "FOVToggle",
	Callback = function(Value)
