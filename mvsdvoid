local FloatEnabled = false
local FloatHeight = 0
local BodyPosition

local function EnableFloat()
	local root = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
	FloatHeight = root.Position.Y

	BodyPosition = Instance.new("BodyPosition")
	BodyPosition.MaxForce = Vector3.new(0, 1e5, 0)
	BodyPosition.P = 1e4
	BodyPosition.D = 100
	BodyPosition.Position = Vector3.new(root.Position.X, FloatHeight, root.Position.Z)
	BodyPosition.Parent = root

	game:GetService("RunService").RenderStepped:Connect(function()
		if FloatEnabled then
			BodyPosition.Position = Vector3.new(root.Position.X, FloatHeight, root.Position.Z)
		else
			BodyPosition:Destroy()
		end
	end)
end

Tab3:CreateToggle({
	Name = "Float",
	CurrentValue = false,
	Flag = "FloatToggle",
	Callback = function(Value)
		FloatEnabled = Value
		if FloatEnabled then EnableFloat() end
	end
})

Tab3:CreateSlider({
	Name = "Float Height",
	Range = {0, 100},
	Increment = 1,
	CurrentValue = 0,
	Flag = "FloatHeightSlider",
	Callback = function(Value)
		FloatHeight = Value
	end
})


