	Name = "Teleport to Pad1",
	Description = "Toggle to teleport to Pad1",
	CurrentValue = false,
	Callback = function(Value)
		Toggles["Pad1"] = Value
		if Value then
			task.spawn(function() TeleportLoop("Pad1") end)
		end
	end
}, "Toggle_Pad1")

